{
  "name": "riku",
  "version": "0.0.4",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "description": "A powerful framework interacting with Discord.js",
  "private": false,
  "scripts": {
    "lint": "eslint \"**/*.ts*\"",
    "build": "npm run build:main && npm run build:cli",
    "build:main": "tsup src/index.ts --format cjs --dts --minify",
    "build:cli": "tsup --entry.cli src/cli/index.ts --minify --format cjs",
    "dev": "npm run build --watch",
    "riku": "riku"
  },
  "bin": {
    "riku": "./dist/cli.js"
  },
  "author": "Foup <fouptogo@gmail.com>",
  "bugs": {
    "url": "https://github.com/rikujs/riku"
  },
  "peerDependencies": {
    "discord.js": "^14.6.0"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.4",
    "axios": "^1.1.3",
    "bundle-require": "^3.1.0",
    "chalk": "5.1.2",
    "commander": "^9.4.1",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "joycon": "^3.1.1",
    "joycon-ts-loader": "^3.0.2",
    "nodemon": "^2.0.20",
    "strip-json-comments": "^5.0.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig": "*",
    "tsconfig-paths": "4.1.0",
    "tsup": "^6.3.0",
    "typescript": "^4.5.2",
    "util": "^0.12.5",
    "uuid": "^9.0.0"
  },
  "keywords": [
    "discord",
    "riku"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
