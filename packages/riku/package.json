{
  "name": "riku",
  "version": "0.0.5",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "discordjs.ts",
    "discordjs.js",
    "discordjs.d.ts",
    "utils.ts",
    "utils.js",
    "utils.d.ts",
    "ui.ts",
    "ui.js",
    "ui.d.ts",
    "config.ts",
    "config.js",
    "config.d.ts"
  ],
  "license": "MIT",
  "description": "A powerful framework interacting with Discord.js",
  "private": false,
  "scripts": {
    "lint": "eslint \"**/*.ts*\"",
    "build": "npm run build:main && npm run build:utils && npm run build:ui && npm run build:discordjs && npm run build:cli",
    "build:main": "tsup src/index.ts --format cjs --dts --minify --treeshake",
    "build:utils": "tsup utils.ts --outDir . --format cjs --dts --minify --treeshake",
    "build:ui": "tsup ui.ts --outDir . --format cjs --dts --minify --treeshake",
    "build:discordjs": "tsup discordjs.ts --outDir . --format cjs --dts --minify --treeshake",
    "build:config": "tsup config.ts --outDir . --format cjs --dts --minify --treeshake",
    "build:cli": "tsup --entry.cli src/cli/index.ts --minify --format cjs && npm run build:spack",
    "build:spack": "tsup --entry.spack-config src/cli/helpers/spack-config.ts --minify --format cjs",
    "prepublishOnly": "npm run build",
    "dev": "npm run build --watch",
    "riku": "riku"
  },
  "bin": {
    "riku": "./dist/cli.js"
  },
  "author": "Foup <fouptogo@gmail.com>",
  "bugs": {
    "url": "https://github.com/rikujs/riku"
  },
  "peerDependencies": {
    "discord.js": "^14.6.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.14",
    "@types/uuid": "^8.3.4",
    "axios": "^1.1.3",
    "chalk": "5.1.2",
    "commander": "^9.4.1",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "find-up": "^6.3.0",
    "json5": "^2.2.1",
    "nodemon": "^2.0.20",
    "ts-node-dev": "^2.0.0",
    "tsconfig": "*",
    "tsconfig-paths": "4.1.0",
    "tsup": "^6.3.0",
    "typescript": "^4.5.2",
    "util": "^0.12.5",
    "uuid": "^9.0.0"
  },
  "keywords": [
    "discord",
    "riku"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
