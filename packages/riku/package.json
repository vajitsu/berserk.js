{
  "name": "riku",
  "version": "0.0.6",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "discordjs.ts",
    "discordjs.js",
    "discordjs.d.ts",
    "utils.ts",
    "utils.js",
    "utils.d.ts",
    "ui.ts",
    "ui.js",
    "ui.d.ts",
    "config.ts",
    "config.js",
    "config.d.ts"
  ],
  "license": "MIT",
  "description": "A powerful framework interacting with Discord.js",
  "private": false,
  "scripts": {
    "lint": "eslint \"**/*.ts*\"",
    "build": "npm run build:main && npm run build:utils && npm run build:ui && npm run build:cli",
    "build:main": "tsup src/index.ts --target es5 --minify --keep-names",
    "build:utils": "tsup utils.ts --outDir . --target es5 --dts --minify --keep-names",
    "build:ui": "tsup ui.ts --outDir . --target es5 --dts --minify --keep-names",
    "build:config": "tsup config.ts --outDir . --target es5 --dts --minify --keep-names",
    "build:cli": "tsup --entry.cli src/cli/index.ts --minify --format cjs && npm run build:spack ",
    "build:spack": "tsup --entry.spack-config src/helpers/spack-config.ts --minify --target es5 --keep-names",
    "prepublishOnly": "npm run build",
    "dev": "npm run build --watch",
    "changelog": "git cliff --prepend ./CHANGELOG.md -u -c ./cliff.toml -r ../../ --include-path 'packages/riku/*'",
    "release": "cliff-jumper --mono-repo"
  },
  "bin": {
    "riku": "./dist/cli.js"
  },
  "author": "Foup <fouptogo@gmail.com>",
  "bugs": {
    "url": "https://github.com/rikujs/riku"
  },
  "peerDependencies": {
    "discord.js": "^14.6.0"
  },
  "devDependencies": {
    "@favware/cliff-jumper": "^1.8.8",
    "@types/uuid": "^8.3.4",
    "axios": "^1.1.3",
    "chalk": "5.1.2",
    "commander": "^9.4.1",
    "eslint": "^8.26.0",
    "eslint-config-custom": "*",
    "find-up": "^6.3.0",
    "json5": "^2.2.1",
    "node-watch": "^0.7.3",
    "tsconfig": "*",
    "tsconfig-paths": "4.1.0",
    "tsup": "^6.4.0",
    "typescript": "^4.5.2",
    "util": "^0.12.5",
    "uuid": "^9.0.0"
  },
  "keywords": [
    "discord",
    "riku"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
