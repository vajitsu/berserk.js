{
  "include": ["src"],
  "exclude": ["dist", "build", "node_modules"],
  "compilerOptions": {
    "target": "ESNext",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "command": ["src/managers/commands/command.ts"],
      "utils": ["src/utils/index.ts"],
      "event": ["src/managers/events/event.ts"],
      "index": ["src/index.ts"],
      "presets/*": ["src/presets/*"]
    }
  }
}
