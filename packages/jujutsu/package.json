{
  "name": "jujutsu",
  "version": "0.0.0",
  "description": "The Discord.js Framework",
  "scripts": {
    "build": "npm run build:main && npm run build:utils && npm run build:ui && npm run build:cli",
    "build:main": "npx spack && node helpers/fix-require-default.js",
    "build:main-types": "tsup src/index.ts --outDir dist --dts-only --dts-resolve",
    "build:utils": "tsup utils.ts --outDir . --dts --minify --keep-names",
    "build:ui": "npm run build:slashcommand && npm run build:event && npm run build:selectmenu && npm run build:button && npm run build:actionrow && npm run build:interactive-button && npm run build:modal",
    "build:cli": "tsup --entry.cli src/cli/index.ts --minify --format cjs",
    "build:slashcommand": "tsup --entry.slash-command builders/slash-command.ts --outDir builders --dts-resolve",
    "build:event": "tsup --entry.event builders/event.ts --outDir builders --dts-resolve",
    "build:selectmenu": "tsup --entry.select-menu builders/select-menu.ts --outDir builders --dts-resolve",
    "build:button": "tsup --entry.button builders/button.ts --outDir builders --dts-resolve",
    "build:interactive-button": "tsup --entry.interactive-button builders/interactive-button.ts --outDir builders --dts-resolve",
    "build:actionrow": "tsup --entry.action-row builders/action-row.ts --outDir builders --dts-resolve",
    "build:modal": "tsup --entry.modal builders/modal.ts --outDir builders --dts-resolve",
    "build:context-menu": "tsup --entry.context-menu builders/context-menu.ts --outDir builders --dts-resolve",
    "prepublishOnly": "npm run build",
    "dev": "npm run build --watch",
    "changelog": "git cliff --prepend ./changelog.md -u -c ./cliff.toml -r ../../ --include-path 'packages/jujutsu/*'",
    "release": "cliff-jumper --mono-repo --tag-template \"{{name}}@{{new-version}}\""
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "builders",
    "utils.ts",
    "utils.js",
    "utils.d.ts",
    "geist.ts",
    "geist.js",
    "geist.d.ts"
  ],
  "bin": {
    "jujutsu": "./dist/cli.js"
  },
  "contributors": [
    "Foup <fouptogo@gmail.com>"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "discord",
    "discordapp",
    "bot",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vajitsu/jujutsu.js.git"
  },
  "bugs": {
    "url": "https://github.com/vajitsu/jujutsu.js/issues"
  },
  "homepage": "https://vajitsu.com/jujutsu",
  "peerDependencies": {
    "discord.js": "^14.6.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/gradient-string": "^1.1.2",
    "@types/lodash": "^4.14.188",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^18.11.9",
    "@types/recursive-readdir": "^2.2.1",
    "@types/uuid": "^8.3.4",
    "@types/watchpack": "^2.4.0",
    "axios": "^1.1.3",
    "chalk": "5.1.2",
    "chokidar": "^3.5.3",
    "commander": "^9.4.1",
    "discord-api-types": "^0.37.17",
    "events": "^3.3.0",
    "find-up": "^6.3.0",
    "fs-extra": "^10.1.0",
    "gradient-string": "^2.0.2",
    "json5": "^2.2.1",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "node-cache": "^5.1.2",
    "node-watch": "^0.7.3",
    "recursive-fs": "^2.1.0",
    "recursive-readdir": "^2.2.3",
    "rimraf": "^3.0.2",
    "rmdir": "^1.2.0",
    "tree-kill": "^1.2.2",
    "tsconfig": "*",
    "tsconfig-paths": "4.1.0",
    "typescript": "latest",
    "undici": "^5.12.0",
    "util": "^0.12.5",
    "uuid": "^9.0.0",
    "watchpack": "^2.4.0"
  },
  "engines": {
    "node": ">=16.9.0"
  },
  "dependencies": {
    "@swc/core": "^1.3.16",
    "tsup": "^6.4.0"
  }
}
