{
  "name": "jujutsu",
  "version": "0.0.0",
  "description": "The Discord.js Framework",
  "scripts": {
    "build": "npm run build:main && npm run build:utils && npm run build:ui && npm run build:cli",
    "build:main": "tsup src/index.ts --target es5 --minify --keep-names",
    "build:utils": "tsup utils.ts --outDir . --target es5 --dts --minify --keep-names",
    "build:ui": "tsup ui.ts --outDir . --target es5 --dts --minify --keep-names",
    "build:config": "tsup config.ts --outDir . --target es5 --dts --minify --keep-names",
    "build:cli": "tsup --entry.cli src/cli/index.ts --minify --format cjs",
    "prepublishOnly": "npm run build",
    "dev": "npm run build --watch",
    "changelog": "git cliff --prepend ./CHANGELOG.md -u -c ./cliff.toml -r ../../ --include-path 'packages/jujutsu/*'",
    "release": "cliff-jumper --mono-repo --tag-template \"{{name}}@{{new-version}}\""
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "utils.ts",
    "utils.js",
    "utils.d.ts",
    "ui.ts",
    "ui.js",
    "ui.d.ts",
    "config.ts",
    "config.js",
    "config.d.ts",
    "geist.ts",
    "geist.js",
    "geist.d.ts"
  ],
  "bin": {
    "jujutsu": "./dist/cli.js"
  },
  "contributors": [
    "Foup <fouptogo@gmail.com>"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "discord",
    "discordapp",
    "bot",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vajitsu/jujutsu.js.git"
  },
  "bugs": {
    "url": "https://github.com/vajitsu/jujutsu.js/issues"
  },
  "homepage": "https://vajitsu.com/jujutsu",
  "peerDependencies": {
    "discord.js": "^14.6.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/uuid": "^8.3.4",
    "axios": "^1.1.3",
    "chalk": "5.1.2",
    "commander": "^9.4.1",
    "find-up": "^6.3.0",
    "json5": "^2.2.1",
    "node-watch": "^0.7.3",
    "tsconfig": "*",
    "tsconfig-paths": "4.1.0",
    "typescript": "latest",
    "util": "^0.12.5",
    "uuid": "^9.0.0"
  },
  "engines": {
    "node": ">=16.9.0"
  }
}
