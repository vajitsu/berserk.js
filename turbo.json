{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", ".mizu/**"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["build", "lint"],
      "outputs": [],
      "cache": false
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": [
    ".env"
  ]
}
