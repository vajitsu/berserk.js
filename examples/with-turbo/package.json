{
  "name": "with-turbo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "^2.7.1",
    "tsconfig-paths": "^4.1.0",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.6.0"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "riku": "latest"
  }
}
