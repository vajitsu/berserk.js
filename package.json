{
  "name": "jujutsu",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --color",
    "dev": "turbo run dev --parallel --color",
    "lint": "turbo run lint",
    "test": "turbo run test --parallel",
    "test:watch": "turbo run test:watch --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "release": "turbo run build --filter=jujutsu --filter=create-jujutsu-app && changeset version && changeset publish && git push --follow-tags",
    "prerelease:jujutsu": "turbo run build --filter=jujutsu",
    "release:jujutsu": "changeset version && changeset publish",
    "prerelease:cra": "turbo run build --filter=create-jujutsu-app",
    "release:cra": "changeset version && changeset publish"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.14",
    "@types/config": "^3.3.0",
    "eslint-config-custom": "*",
    "prettier": "^2.7.1",
    "sass": "^1.56.0",
    "stylelint": "^14.14.1",
    "stylelint-prettier": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "tsup": "^6.3.0",
    "turbo": "latest",
    "vitest": "^0.24.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.19.2",
  "dependencies": {
    "@changesets/cli": "^2.25.2",
    "@code-hike/mdx": "^0.7.4",
    "@mdx-js/loader": "^2.1.5",
    "@next/font": "^13.0.2",
    "@next/mdx": "^13.0.2",
    "@sapphire/shapeshift": "^3.7.0",
    "chalk": "^5.1.2",
    "config": "^3.3.8",
    "create-jujutsu-app": "*",
    "dotenv": "^16.0.3",
    "geist": "^0.1.2",
    "jujutsu": "*",
    "next": "^13.0.2",
    "node-watch": "^0.7.3"
  }
}
