{
  "name": "berserk-project",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "create-berserk-app": "node --trace-deprecation --enable-source-maps packages/create-berserk-app/dist/index.js",
    "berserk": "node --trace-deprecation --enable-source-maps packages/berserk/dist/bin/berserk",
    "lint-typescript": "turbo run typescript",
    "lint-eslint": "eslint . --ext js,jsx,ts,tsx --max-warnings=0 --config .eslintrc.json --no-eslintrc",
    "lint-no-typescript": "run-p prettier-check lint-eslint",
    "lint": "run-p lint-typescript prettier-check lint-eslint",
    "lint-fix": "pnpm prettier-fix && eslint . --ext js,jsx,ts,tsx --fix --max-warnings=0 --config .eslintrc.json --no-eslintrc",
    "prettier-check": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "test": "turbo run test --parallel",
    "types": "lerna run types --stream",
    "typescript": "turbo run typescript",
    "prepublishOnly": "turbo run build",
    "publish-canary": "git checkout canary && git pull && lerna version prerelease --preid canary --force-publish && pnpm release --pre --skip-questions",
    "publish-stable": "lerna version --force-publish",
    "publish-pkgs": "lerna publish from-package",
    "lint-staged": "lint-staged",
    "version": "pnpm install && git add pnpm-lock.yaml",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "7.18.0",
    "@babel/eslint-parser": "7.18.2",
    "@babel/generator": "7.18.0",
    "@berserk/release": "workspace:*",
    "@changesets/cli": "^2.25.2",
    "@sapphire/shapeshift": "^3.7.0",
    "@swc/cli": "0.1.55",
    "@swc/core": "1.2.203",
    "@swc/helpers": "0.4.14",
    "@taskr/clear": "1.1.0",
    "@taskr/esnext": "1.1.0",
    "@taskr/watch": "1.1.0",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.29.1",
    "@vercel/ncc": "0.36.0",
    "alex": "11.0.0",
    "arg": "^5.0.2",
    "async-sema": "^3.1.1",
    "berserk": "workspace:*",
    "discord.js": "14.6.0",
    "dotenv": "^16.0.3",
    "eslint": "7.24.0",
    "eslint-plugin-eslint-plugin": "5.0.6",
    "eslint-plugin-import": "2.22.1",
    "find-up": "^6.3.0",
    "husky": "8.0.0",
    "lerna": "4.0.0",
    "lint-staged": "10.1.7",
    "mintlify": "1.2.0",
    "module": "1.2.5",
    "next": "13.0.8-canary.0",
    "node-notifier": "10.0.1",
    "npm-run-all": "4.1.5",
    "path": "^0.12.7",
    "prettier": "^2.7.1",
    "shell-quote": "1.7.3",
    "tsconfig-paths": "^4.1.1",
    "turbo": "1.6.3",
    "typescript": "4.9.3"
  },
  "packageManager": "pnpm@7.3.0",
  "engines": {
    "node": ">=14.6.0"
  }
}
