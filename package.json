{
  "name": "jujutsu-project",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "jujutsu": "node --trace-deprecation --enable-source-maps packages/jujutsu/dist/bin/jujutsu",
    "lint": "turbo run lint",
    "prepare  ": "husky install",
    "prepublishOnly": "turbo run build",
    "prettier-check": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "publish-stable": "lerna version --force-publish",
    "test": "turbo run test --parallel",
    "types": "lerna run types --stream",
    "typescript": "turbo run typescript",
    "version": "pnpm install && git add pnpm-lock.yaml"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "@changesets/cli": "^2.25.2",
    "@sapphire/shapeshift": "^3.7.0",
    "@swc/cli": "^0.1.57",
    "@taskr/clear": "1.1.0",
    "@taskr/esnext": "1.1.0",
    "@taskr/watch": "1.1.0",
    "@vercel/ncc": "0.34.0",
    "@vercel/style-guide": "4.0.2",
    "arg": "^5.0.2",
    "async-sema": "^3.1.1",
    "chalk": "^5.1.2",
    "create-jujutsu-app": "workspace:*",
    "discord.js": "14.6.0",
    "dotenv": "^16.0.3",
    "eslint": "7.24.0",
    "eslint-config-custom": "*",
    "find-up": "^6.3.0",
    "husky": "8.0.0",
    "jujutsu": "workspace:0.0.0",
    "lerna": "4.0.0",
    "lint-staged": "10.1.7",
    "module": "1.2.5",
    "next": "13.0.4",
    "node-notifier": "10.0.1",
    "ora": "6.1.2",
    "path": "^0.12.7",
    "prettier": "^2.7.1",
    "shell-quote": "1.7.3",
    "taskr": "1.1.0",
    "tsconfig-paths": "^4.1.0",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.3.0",
  "engines": {
    "node": ">=14.6.0"
  }
}
